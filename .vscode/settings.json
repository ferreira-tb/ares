{
    "vsicons.associations.files": [
        {
            "icon": "vite",
            "extensions": ["browser.config.ts", "modules.config.ts", "panel.config.ts", "ui.config.ts"],
            "filename": true,
            "format": "svg"
        }
    ],
    "vsicons.associations.folders": [
        { "icon": "src", "extensions": ["vue"], "format": "svg" },
        { "icon": "api", "extensions": ["browser"], "format": "svg" },
        { "icon": "aurelia", "extensions": ["ipc"], "format": "svg" },
        { "icon": "ravendb", "extensions": ["worker"], "format": "svg" },
        { "icon": "story", "extensions": ["store", "stores"], "format": "svg" },
        { "icon": "nix", "extensions": ["panel"], "format": "svg" },
        { "icon": "theme", "extensions": ["composable", "composables"], "format": "svg" }
    ],
    "conventionalCommits.scopes": [
        "browser"
    ],
    "conventionalCommits.promptScopes": false,
    "files.exclude": {
        "**/.vitepress/cache": true
    },
    "[markdown]": {
        "editor.tabSize": 2
    }
}