{
    "name": "ares",
    "productName": "Ares",
    "version": "1.0.0-alpha.6",
    "description": "Ferramenta para Tribal Wars",
    "author": {
        "name": "Andrew Ferreira",
        "email": "andrew.shien2@gmail.com",
        "url": "https://github.com/ferreira-tb"
    },
    "homepage": "https://github.com/ferreira-tb/ares",
    "bugs": {
        "url": "https://github.com/ferreira-tb/ares/issues",
        "email": "andrew.shien2@gmail.com"
    },
    "license": "MIT",
    "main": "dist/ares.js",
    "keywords": [
        "tribal-wars",
        "electron"
    ],
    "scripts": {
        "type-check": "vue-tsc --noEmit",
        "build": "npm run type-check && npm-run-all build:* && node scripts/html.mjs",
        "build:panel": "vite build --config vite.config.ts",
        "build:mod": "vite build --config modules.config.ts",
        "build:browser": "vite build --config browser.config.ts",
        "build:phobos": "vite build --config phobos.config.ts",
        "panel": "npm run type-check && npm run build:panel && node scripts/html.mjs",
        "mod": "npm run type-check && npm run build:mod && node scripts/html.mjs",
        "browser": "npm run type-check && npm run build:browser",
        "phobos": "npm run type-check && npm run build:phobos.",
        "main": "rollup -c && electron .",
        "start": "npm run build && npm run main",
        "test": "vitest",
        "test:ui": "vitest --ui"
    },
    "dependencies": {
        "@sequelize/core": "^7.0.0-alpha.21",
        "@vueuse/core": "^9.12.0",
        "@vueuse/electron": "^9.12.0",
        "electron-store": "^8.1.0",
        "pinia": "^2.0.30",
        "sqlite3": "^5.1.4",
        "vue": "^3.2.47",
        "vue-router": "^4.1.6",
        "vuetify": "^3.1.4"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^24.0.0",
        "@rollup/plugin-json": "^6.0.0",
        "@rollup/plugin-node-resolve": "^15.0.1",
        "@rollup/plugin-typescript": "^11.0.0",
        "@types/jsdom": "^21.1.0",
        "@types/node": "^18.11.18",
        "@vitejs/plugin-vue": "^4.0.0",
        "@vitest/ui": "^0.28.4",
        "@vue/test-utils": "^2.2.10",
        "@vue/tsconfig": "^0.1.3",
        "electron": "^23.0.0",
        "jsdom": "^21.1.0",
        "npm-run-all": "^4.1.5",
        "pg-hstore": "^2.3.4",
        "tslib": "^2.5.0",
        "typescript": "^4.9.5",
        "vite": "^4.1.1",
        "vitest": "^0.28.4",
        "vue-tsc": "^1.0.12"
    }
}