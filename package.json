{
    "name": "ares",
    "productName": "Ares",
    "version": "1.0.0-alpha.23",
    "description": "Ferramenta para Tribal Wars",
    "author": {
        "name": "Andrew Ferreira",
        "email": "andrew.shien2@gmail.com",
        "url": "https://github.com/ferreira-tb"
    },
    "homepage": "https://tb.dev.br/ares",
    "bugs": {
        "url": "https://github.com/ferreira-tb/ares/issues",
        "email": "andrew.shien2@gmail.com"
    },
    "license": "MIT",
    "main": "dist/ares.js",
    "scripts": {
        "type-check": "vue-tsc --noEmit",
        "clear": "node scripts/clear.mjs",
        "build": "npm run clear && npm run type-check && npm-run-all build:* && npm run script",
        "build:main": "vite build --config main.config.ts",
        "build:browser": "vite build --config browser.config.ts",
        "build:mod": "vite build --config modules.config.ts",
        "build:panel": "vite build --config panel.config.ts",
        "build:rollup": "rollup -c",
        "script": "npm-run-all script:*",
        "script:css": "node scripts/css.mjs",
        "script:html": "node scripts/html.mjs",
        "script:fonts": "node scripts/fonts.mjs",
        "start": "npm run build && electron .",
        "start:main": "npm run build:main && npm run script:html && npm run script:fonts && electron .",
        "start:panel": "npm run build:panel && npm run script:html && electron .",
        "start:rollup": "npm run build:rollup && electron .",
        "forge:start": "electron-forge start",
        "forge:package": "electron-forge package",
        "forge:make": "electron-forge make",
        "forge:publish": "npm run build && electron-forge make"
    },
    "dependencies": {
        "@tb-dev/prototype": "^2.0.2",
        "@tb-dev/prototype-dom": "^3.0.1",
        "@tb-dev/ts-guard": "^2.0.0",
        "@tb-dev/ts-guard-dom": "^1.0.0",
        "@vueuse/core": "^9.13.0",
        "@vueuse/electron": "^9.13.0",
        "mechanus": "^1.0.5",
        "pinia": "^2.0.33",
        "sequelize": "^6.30.0",
        "sqlite3": "^5.1.6",
        "vue": "^3.2.47",
        "vue-router": "^4.1.6"
    },
    "devDependencies": {
        "@electron-forge/cli": "^6.0.5",
        "@electron-forge/maker-zip": "^6.0.5",
        "@rollup/plugin-commonjs": "^24.0.1",
        "@rollup/plugin-json": "^6.0.0",
        "@rollup/plugin-node-resolve": "^15.0.1",
        "@rollup/plugin-typescript": "^11.0.0",
        "@types/node": "^18.15.3",
        "@vicons/carbon": "^0.12.0",
        "@vicons/fa": "^0.12.0",
        "@vicons/ionicons5": "^0.12.0",
        "@vicons/material": "^0.12.0",
        "@vitejs/plugin-vue": "^4.1.0",
        "@vue/tsconfig": "^0.1.3",
        "electron": "^23.2.0",
        "naive-ui": "^2.34.3",
        "npm-run-all": "^4.1.5",
        "pg-hstore": "^2.3.4",
        "rollup": "^3.20.2",
        "sass": "^1.60.0",
        "tslib": "^2.5.0",
        "typescript": "^4.9.5",
        "vite": "^4.2.1",
        "vue-tsc": "^1.2.0"
    }
}