        "@rollup/plugin-typescript": "^11.0.0",
        "@types/node": "^18.11.12",
        "@vitejs/plugin-vue": "^4.0.0",
        "@vue/tsconfig": "^0.1.3",
        "electron": "^22.0.2",
        "npm-run-all": "^4.1.5",
        "tslib": "^2.4.1",
        "typescript": "^4.9.4",
        "vite": "^4.0.0",
        "vue-tsc": "^1.0.12"
