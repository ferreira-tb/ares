{
    "name": "claustrophobia",
    "version": "1.0.0",
    "description": "Ferramenta para Tribal Wars",
    "main": "dist/main.js",
    "scripts": {
        "dev": "vite",
        "build": "run-p type-check build-only && node dist.mjs",
