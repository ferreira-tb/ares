{
    "name": "ares",
    "productName": "Ares",
    "version": "1.0.0-alpha.15",
    "description": "Ferramenta para Tribal Wars",
    "author": {
        "name": "Andrew Ferreira",
        "email": "andrew.shien2@gmail.com",
        "url": "https://github.com/ferreira-tb"
    },
    "homepage": "https://tb.dev.br/ares",
    "bugs": {
        "url": "https://github.com/ferreira-tb/ares/issues",
        "email": "andrew.shien2@gmail.com"
    },
    "license": "MIT",
    "main": "dist/ares.js",
    "keywords": [
        "tribal-wars",
        "electron"
    ],
    "scripts": {
        "type-check": "vue-tsc --noEmit",
        "build": "npm run type-check && npm-run-all build:* && npm run script",
        "build:panel": "vite build --config vite.config.ts",
        "build:mod": "vite build --config modules.config.ts",
        "build:browser": "vite build --config browser.config.ts",
        "panel": "npm run type-check && npm run build:panel && node scripts/html.mjs",
        "mod": "npm run type-check && npm run build:mod && node scripts/html.mjs",
        "browser": "npm run type-check && npm run build:browser && node scripts/css.mjs",
        "main": "rollup -c && electron .",
        "clear": "node scripts/clear.mjs",
        "script": "npm-run-all script:*",
        "script:css": "node scripts/css.mjs",
        "script:html": "node scripts/html.mjs",
        "start": "npm run clear && npm run build && npm run main",
        "forge:start": "electron-forge start",
        "forge:package": "electron-forge package",
        "forge:make": "electron-forge make"
    },
    "dependencies": {
        "@tb-dev/prototype": "^2.0.1",
        "@tb-dev/prototype-dom": "^3.0.0",
        "@tb-dev/ts-guard": "^2.0.0",
        "@tb-dev/ts-guard-dom": "^1.0.0",
        "@vueuse/core": "^9.13.0",
        "@vueuse/electron": "^9.13.0",
        "pinia": "^2.0.32",
        "sequelize": "^6.29.0",
        "sqlite3": "^5.1.4",
        "vue": "^3.2.47",
        "vue-router": "^4.1.6"
    },
    "devDependencies": {
        "@electron-forge/cli": "^6.0.5",
        "@electron-forge/maker-zip": "^6.0.5",
        "@rollup/plugin-commonjs": "^24.0.1",
        "@rollup/plugin-json": "^6.0.0",
        "@rollup/plugin-node-resolve": "^15.0.1",
        "@rollup/plugin-typescript": "^11.0.0",
        "@types/node": "^18.14.4",
        "@vitejs/plugin-vue": "^4.0.0",
        "@vue/tsconfig": "^0.1.3",
        "electron": "^23.1.1",
        "naive-ui": "^2.34.3",
        "npm-run-all": "^4.1.5",
        "pg-hstore": "^2.3.4",
        "sass": "^1.58.3",
        "tslib": "^2.5.0",
        "typescript": "^4.9.5",
        "vite": "^4.1.4",
        "vue-tsc": "^1.2.0"
    }
}