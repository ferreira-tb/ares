        "@rollup/plugin-json": "^6.0.0",
        "@rollup/plugin-node-resolve": "^15.1.0",
        "@rollup/plugin-typescript": "^11.1.2",
        "@tb-dev/kronos": ">=1.0.1",
        "@tb-dev/prototype": ">=3.0.0",
        "@tb-dev/prototype-dom": ">=5.0.1",
        "@types/cls-hooked": "^4.3.4",
        "@types/node": "^20.4.8",
        "@types/semver": "^7.5.0",
        "@typescript-eslint/eslint-plugin": "^5.62.0",
        "@typescript-eslint/parser": "^5.62.0",
        "@vicons/carbon": "^0.12.0",
        "@vicons/fa": "^0.12.0",
        "@vicons/ionicons5": "^0.12.0",
        "@vicons/material": "^0.12.0",
        "@vitejs/plugin-vue": "^4.2.3",
        "@vue/tsconfig": "^0.4.0",
        "@vueuse/core": "^10.3.0",
        "@vueuse/electron": "^10.3.0",
        "@vueuse/router": "^10.3.0",
        "cls-hooked": "^4.2.2",
        "csv-parser": "^3.0.0",
        "discord.js": "^14.12.1",
        "electron": ">=25.4.0",
        "electron-store": "^8.1.0",
        "eslint": "^8.46.0",
        "eslint-plugin-vue": "^9.16.1",
        "lodash-es": "^4.17.21",
        "mechanus": "^3.6.4",
        "naive-ui": "^2.34.4",
        "npm-run-all": "^4.1.5",
        "pg-hstore": "^2.3.4",
        "pinia": "^2.1.6",
        "rollup": "^3.27.2",
        "sass": "^1.64.2",
        "semver": "^7.5.4",
        "taze": "^0.11.2",
        "tslib": "^2.6.1",
        "typescript": "^5.1.6",
        "vite": "^4.4.8",
        "vitepress": "^1.0.0-beta.7",
